{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./src/*"],
      "@chat-sdk-ui/*": ["./src/sdk/chat-sdk-ui/*"],
      "@chat-sdk-backend-client/*": ["./src/sdk/chat-sdk-backend-client/*"],
      "@chat-sdk-offline-client/*": ["./src/sdk/chat-sdk-offline/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
